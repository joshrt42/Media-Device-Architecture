    for (u8 i = 0; i < 9; i++)
        drawCheese(&cheese[i]);

    for (u8 i = 0; i < 6; i++) {
        drawCat(&cats[i]);
    }

    for (u8 i = 0; i < 5; i++)
        drawBullet(&bullets[i]);
}







