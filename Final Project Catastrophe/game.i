void initGame() {

    initPlayer();
    initCats();
    initCheese();
    initBullets();

    gameOver = 0;
    gameWon = 0;
    timeRemaining = 30;
    timeRemainingCounter = 0;
    cheeseRemaining = 9;
    enemiesRemaining = 6;
}


void updateGame() {

    updatePlayer();

    for (u8 i = 0; i < 6; i++)
        updateCat(&cats[i]);

    for (u8 i = 0; i < 9; i++)
        updateCheese(&cheese[i]);

    for (u8 i = 0; i < 5; i++)
        updateBullet(&bullets[i]);

    if (timeRemainingCounter++ > 59) {
        if (timeRemaining <= 0) {
            gameOver = 1;
        }
        timeRemainingCounter = 0;
        timeRemaining -= 1;
    }

    if (cheeseRemaining == 0 && player.worldCol < 24 && player.worldRow > 110) {
        gameWon = 1;
    }
}


