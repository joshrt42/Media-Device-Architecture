            player.worldCol -= player.cdel;
            player.numFrames = 7;
            player.aniState = 1;
            if (player.aniCounter % 3 == 0) {
                player.curFrame = (player.curFrame + 1) % player.numFrames;
            }
            player.aniCounter++;
        } else if ((~((*(volatile unsigned short *)0x04000130)) & ((1<<4))) && player.worldCol < 768 - player.width ) {
            player.facing = 1;
            player.worldCol += player.cdel;
            player.numFrames = 7;
            player.aniState = 1;
            if (player.aniCounter % 3 == 0) {
                player.curFrame = (player.curFrame + 1) % player.numFrames;
            }
            player.aniCounter++;
        } else {
            player.numFrames = 7;
            player.aniState = 0;
            if (player.aniCounter % 12 == 0) {
                player.curFrame = (player.curFrame + 1) % player.numFrames;
            }
            player.aniCounter++;
        }

        if ((~((*(volatile unsigned short *)0x04000130)) & ((1<<6))) && player.worldRow > 134) {
            player.rdel = -44;
        }

        if (player.rdel < 0) {
            player.worldRow += player.rdel/7;
            player.rdel+=2;
        } else if (player.rdel == 0 && player.worldRow != 135) {
            player.rdel+=2;
        } else if (player.rdel > 0) {
            if (player.worldRow < 134) {
                player.worldRow += player.rdel/7;
                player.rdel+=2;
            } else {
                player.worldRow = 135;
                player.rdel = 0;
            }
        }

        if ((!(~(oldButtons)&((1<<0))) && (~buttons & ((1<<0))))) {
            if (player.hasGun) {
                player.hasGun = 0;
                shadowOAM[gunOAM].attr0 |= (2<<8);
            }
            else {
                player.hasGun = 1;
            }
        }
        if ((!(~(oldButtons)&((1<<1))) && (~buttons & ((1<<1)))) && player.hasGun) {
            playSoundB(soundGun,6616,11025, 0);
            fireBullet();
        }
        if (player.health < 1)
            gameOver = 1;
    }
}


