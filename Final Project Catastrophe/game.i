void initBullets() {

    for (int i = 0; i < 5; i++) {

        bullets[i].height = 1;
        bullets[i].width = 2;
        bullets[i].worldRow = player.worldRow + (player.height/2);
        bullets[i].worldCol = 0;
        bullets[i].screenRow = bullets[i].worldRow;
        bullets[i].screenCol = bullets[i].worldCol;
        bullets[i].cdel = 0;
        bullets[i].active = 0;
        bullets[i].bulletNo = i;
    }
}


void fireBullet() {

    for (int i = 0; i < 5; i++) {
        if (!bullets[i].active) {

            bullets[i].worldRow = player.worldRow + player.height/2;
            if (player.facing) {
                bullets[i].cdel = 3;
                bullets[i].worldCol = player.worldCol + player.width;
                bullets[i].screenCol = player.screenCol + player.width;
            } else {
                bullets[i].cdel = -3;
                bullets[i].worldCol = player.worldCol;
                bullets[i].screenCol = player.screenCol;
            }
            bullets[i].screenRow = bullets[i].worldRow;
            bullets[i].active = 1;

            break;
        }
    }
}


void updateBullet(BULLET* b) {

    if (b->active) {
        b->worldCol += b->cdel;
        if (b->screenCol < 0 || b->screenCol>240) {
            b->active = 0;
            shadowOAM[bulletOAM+b->bulletNo].attr0 |= (2<<8);
        }
    }
}


void drawBullet(BULLET* b) {

    if (b->active) {
        shadowOAM[bulletOAM+b->bulletNo].attr0 = (b->screenRow & 0xFF) | (0<<14) | (0<<8);
        shadowOAM[bulletOAM+b->bulletNo].attr1 = (b->screenCol & 0x1FF) | (0<<14);
        shadowOAM[bulletOAM+b->bulletNo].attr2 = ((15)*32+(4));
    }
}
