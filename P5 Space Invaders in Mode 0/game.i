void initEnemies() {

    for (u8 i = 0; i < 30; i++) {
        enemies[i].height = 8;
        enemies[i].row = 20 + 12 * (i / 6);
        enemies[i].rdel = 5;
        enemies[i].cdel = 1;
        enemies[i].active = 1;
        enemies[i].erased = 0;
        if (30 - i <= 6)
            enemies[i].isBottom = 1;
        if (i < 6) {
            enemies[i].width = 8;
            enemies[i].sprite = 2;
            enemies[i].col = 4 + 16 * (i % 6);
        } else if (i < 24) {
            enemies[i].width = 11;
            enemies[i].sprite = 1;
            enemies[i].col = 2 + 16 * (i % 6);
        } else if (i < 30) {
            enemies[i].width = 12;
            enemies[i].sprite = 0;
            enemies[i].col = 1 + 16 * (i % 6);
        }
        enemies[i].aniCounter = 0;
        enemies[i].curFrame = 0;
        enemies[i].numFrames = 4;
        enemies[i].spriteNo = i + 1;
    }
}


void updateEnemy(ENEMY* e) {

    if (e->active) {

        if (e->spriteNo < 25)
            if (!enemies[(e->spriteNo) + 6].active)
                e->isBottom = 1;

        if (e->isBottom) {
            int shoot = rand() % 250;
            if (shoot == 1) {
                fireEnemyBullet(e);
            }
        }


        for (u8 i = 0; i < 5; i++) {
            if (playerBullets[i].active && collision(e->row, e->col, e->height, e->width, playerBullets[i].row, playerBullets[i].col, playerBullets[i].height, playerBullets[i].width)) {

                playerBullets[i].active = 0;
                e->active = 0;
                shadowOAM[e->spriteNo].attr0 = (2<<8);
                shadowOAM[playerBullets[i].spriteNo].attr0 = (2<<8);
                enemiesRemaining--;
                score++;
                explode(e);
            }
        }


        if (player.active && collision(e->row, e->col, e->height, 12, player.row, player.col, player.height, player.width)) {
            player.active = 0;
            pExplode();
        }


        e->col += e->cdel;


        if(e->aniCounter/145 == 1) {
            e->cdel*=-1;
            e->aniCounter = 0;
            if (e->row < 140) {
                e->row+=e->rdel;
            } else if (player.active) {
                pExplode();
                player.active = 0;
            }
        }
    }


    if (e->aniCounter % 20 == 0) {
        e->curFrame = (e->curFrame + 1) % e->numFrames;
    }
    e->aniCounter++;
}


void drawEnemy(ENEMY* e) {
    if (e->active) {
        shadowOAM[e->spriteNo].attr0 = e->row | (1<<14) | (0<<8);
        shadowOAM[e->spriteNo].attr1 = e->col | (0<<14);
        shadowOAM[e->spriteNo].attr2 = ((e->curFrame)*32+(2 + (e->sprite)*2));
    } else if (!e->erased) {
        e->erased = 1;
    }
}
