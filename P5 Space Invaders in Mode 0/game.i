ENEMYBULLET enemyBullets[5];
ENEMY enemies[30];
BARRIER barriers[4];
EXPLOSION explosion;
PEXPLOSION pExplosion;

int enemiesRemaining = 30;
int gameOver = 0;
int gameWon = 0;
int score = 0;





void initGame() {

    initPlayer();
    initPlayerBullets();
    initEnemyBullets();
    initEnemies();
    initBarriers();
    initExplosion();
    initpExplosion();

    enemiesRemaining = 30;
}


void updateGame() {

    updatePlayer();
    updateExplosion();
    updatepExplosion();

    for (u8 i = 0; i < 5; i++)
        updatePlayerBullet(&playerBullets[i]);


    for (u8 i = 0; i < 5; i++)
        updateEnemyBullet(&enemyBullets[i]);


    for (u8 i = 0; i < 4; i++)
        updateBarrier(&barriers[i]);

    for (u8 i = 0; i < 30; i++)
        updateEnemy(&enemies[i]);

    if (enemiesRemaining == 0)
        gameWon = 1;
}


void drawGame() {

    drawHealth();
    drawScore();
    drawPlayer();
    drawpExplosion();

    for (u8 i = 0; i < 5; i++)
        drawPlayerBullet(&playerBullets[i]);


    for (u8 i = 0; i < 5; i++)
        drawEnemyBullet(&enemyBullets[i]);

    for (u8 i = 0; i < 30; i++)
        drawEnemy(&enemies[i]);

    for (u8 i = 0; i < 4; i++)
        drawBarrier(&barriers[i]);

    drawExplosion();
}







void initPlayer() {
    player.col = 240/2 - 5;
    player.row = 146;
    player.cdel = 2;
    player.height = 8;
    player.width = 11;
    player.active = 1;
    player.bulletTimer = 30;
    player.health = 3;
    player.aniCounter = 0;
    player.curFrame = 0;
    player.numFrames = 4;
}


void updatePlayer() {
    if (player.active) {
        if ((~((*(volatile unsigned short *)0x04000130)) & ((1<<5))) && player.col > 1) {
            player.col -= player.cdel;
        } else if ((~((*(volatile unsigned short *)0x04000130)) & ((1<<4))) && player.col < 240 - player.width ) {
            player.col += player.cdel;
        }


        if ((!(~(oldButtons)&((1<<0))) && (~buttons & ((1<<0)))) && player.bulletTimer >= 30) {
            firePlayerBullet();
        }

        player.bulletTimer++;


        for (u8 i = 0; i < 5; i++)
            if (enemyBullets[i].active && collision(player.row, player.col, player.height, player.width, enemyBullets[i].row, enemyBullets[i].col, enemyBullets[i].height, enemyBullets[i].width)) {

                enemyBullets[i].active = 0;
                player.health--;
                shadowOAM[enemyBullets[i].spriteNo].attr0 = (2<<8);
                if (player.health <= 0) {
                    pExplode();
                }
            }


        if (player.aniCounter % 5 == 0) {
            player.curFrame = (player.curFrame + 1) % player.numFrames;
        }
        player.aniCounter++;

    }
}


void drawPlayer() {

    if (player.active) {
        shadowOAM[0].attr0 = player.row | (1<<14) | (0<<8);
        shadowOAM[0].attr1 = player.col | (0<<14);
        shadowOAM[0].attr2 = ((player.curFrame)*32+(0));
    }

}


void drawHealth() {
    shadowOAM[45].attr0 = 8 | (0<<14) | (0<<8);
    shadowOAM[45].attr1 = 210 | (0<<14);
    shadowOAM[45].attr2 = ((6)*32+(player.health));
}


void drawScore() {
